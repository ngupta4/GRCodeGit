<?xml version="1.0" encoding="UTF-8" ?>
<?demandware-pipeline version="2.0"?>

<pipeline group="MyAccount">
  <branch basename="_ANONYMOUS_BRANCH_1">
    <segment>
      <node>
        <text-node>
          <description>Show the cart page.</description>
        </text-node>
        <node-display x="1" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="Show">
    <segment>
      <node>
        <start-node name="Show" secure="false"/>
        <node-display x="1" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <call-node start-name-ref="mAccount-RequireLogin"/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <jump-node start-name-ref="mCart-Show"/>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_3">
    <segment>
      <node>
        <text-node>
          <description>Creates a new customer account.</description>
        </text-node>
        <node-display x="4" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="CreateAccount">
    <segment>
      <node>
        <start-node call-mode="public" name="CreateAccount" secure="false"/>
        <node-display x="4" y="2"/>
      </node>
      <simple-transition transaction-control="begin">
        <transition-display>
          <bend-point relative-to="target" x="0" y="-1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="true"/>
          <key-binding alias="CurrentForms.profile.customer.email.value" key="From_0"/>
          <key-binding alias="Login" key="To_0"/>
          <key-binding alias="CurrentForms.profile.login.password.value" key="From_1"/>
          <key-binding alias="Password" key="To_1"/>
          <key-binding alias="null" key="From_2"/>
          <key-binding alias="null" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="null" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
        </pipelet-node>
        <node-display x="0" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
          <bend-point relative-to="source" x="-1" y="1"/>
          <bend-point relative-to="target" x="-1" y="0"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="GetCustomer" pipelet-set-identifier="bc_api">
          <key-binding alias="Login" key="Login"/>
          <key-binding alias="null" key="Customer"/>
        </pipelet-node>
        <node-display orientation="horizontal" x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in"/>
          <segment>
            <node>
              <pipelet-node pipelet-name="CreateCustomer" pipelet-set-identifier="bc_api">
                <key-binding alias="Customer" key="Customer"/>
                <key-binding alias="Login" key="Login"/>
                <key-binding alias="Password" key="Password"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
              <branch basename="b2" source-connector="error">
                <transition target-connector="in1" target-path="./b4.3"/>
              </branch>
            </node>
            <simple-transition>
              <transition-display>
                <bend-point relative-to="source" x="0" y="1"/>
              </transition-display>
            </simple-transition>
            <node>
              <pipelet-node pipelet-name="UpdateObjectWithForm" pipelet-set-identifier="bc_api">
                <key-binding alias="CurrentForms.profile.login" key="Form"/>
                <key-binding alias="Customer.profile.credentials" key="Object"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
              <branch basename="b3" source-connector="error">
                <transition target-connector="in1" target-path="./b4.2"/>
              </branch>
            </node>
            <simple-transition>
              <transition-display>
                <bend-point relative-to="source" x="0" y="1"/>
              </transition-display>
            </simple-transition>
            <node>
              <pipelet-node pipelet-name="UpdateObjectWithForm" pipelet-set-identifier="bc_api">
                <key-binding alias="CurrentForms.profile.customer" key="Form"/>
                <key-binding alias="Customer.profile" key="Object"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
              <branch basename="b4" source-connector="error">
                <transition target-connector="in1"/>
                <segment>
                  <node>
                    <join-node/>
                    <node-display x="1" y="0"/>
                  </node>
                  <transition target-connector="in1" target-path="./+1"/>
                </segment>
                <segment>
                  <node>
                    <join-node/>
                    <node-display x="0" y="-1"/>
                  </node>
                  <transition target-connector="in1" target-path="./+1"/>
                </segment>
                <segment>
                  <node>
                    <join-node/>
                    <node-display x="0" y="-1"/>
                  </node>
                  <simple-transition transaction-control="rollback"/>
                  <node>
                    <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
                      <config-property key="Transactional" value="false"/>
                      <config-property key="OnError" value="Exception"/>
                      <config-property key="ScriptFile" value="mobile/AddToStatus.ds"/>
                      <key-binding alias="scriptLog" key="ScriptLog"/>
                      <key-binding alias="status" key="inStatus"/>
                      <key-binding alias="&quot;The system failed to create a new customer. Please try again.&quot;" key="itemCode"/>
                      <key-binding alias="0" key="itemStatus"/>
                      <key-binding alias="status" key="status"/>
                    </pipelet-node>
                    <node-display orientation="horizontal" x="1" y="0"/>
                  </node>
                  <simple-transition>
                    <transition-display>
                      <bend-point relative-to="source" x="1" y="0"/>
                    </transition-display>
                  </simple-transition>
                  <node>
                    <jump-node start-name-ref="mAccount-Register"/>
                    <node-display orientation="horizontal" x="1" y="0"/>
                  </node>
                </segment>
              </branch>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node custom-name="Set ReportingMarker Registration" pipelet-name="Assign" pipelet-set-identifier="bc_api">
                <description>Sets a special reporting marker to indicate a successful registration. This flag will later be evaluate in the template and reset to avoid double reporting of a registration.</description>
                <key-binding alias="&quot;true&quot;" key="From_0"/>
                <key-binding alias="ReportingMarker_SuccessfulRegistration" key="To_0"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node pipelet-name="LoginCustomer" pipelet-set-identifier="bc_api">
                <key-binding alias="Login" key="Login"/>
                <key-binding alias="Password" key="Password"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
              <branch basename="b5" source-connector="error">
                <transition target-connector="in" transaction-control="rollback">
                  <transition-display>
                    <bend-point relative-to="source" x="1" y="0"/>
                  </transition-display>
                </transition>
                <segment>
                  <node>
                    <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
                      <config-property key="Transactional" value="false"/>
                      <config-property key="OnError" value="Exception"/>
                      <config-property key="ScriptFile" value="mobile/AddToStatus.ds"/>
                      <key-binding alias="scriptLog" key="ScriptLog"/>
                      <key-binding alias="status" key="inStatus"/>
                      <key-binding alias="&quot;The pipelet 'LoginCustomer' failed&quot;" key="itemCode"/>
                      <key-binding alias="0" key="itemStatus"/>
                      <key-binding alias="status" key="status"/>
                    </pipelet-node>
                    <node-display orientation="horizontal" x="1" y="0"/>
                  </node>
                  <simple-transition>
                    <transition-display>
                      <bend-point relative-to="source" x="1" y="0"/>
                    </transition-display>
                  </simple-transition>
                  <node>
                    <jump-node start-name-ref="mAccount-Register"/>
                    <node-display orientation="horizontal" x="1" y="0"/>
                  </node>
                </segment>
              </branch>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
                <key-binding alias="CurrentForms.profile" key="FormElement"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <interaction-node transaction-required="false">
                <template buffered="true" dynamic="false" name="mobile/account/accountoverview"/>
              </interaction-node>
              <node-display x="0" y="1"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition transaction-control="rollback">
        <transition-display>
          <bend-point relative-to="target" x="-1" y="0"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="Exception"/>
          <config-property key="ScriptFile" value="mobile/AddToStatus.ds"/>
          <key-binding alias="scriptLog" key="ScriptLog"/>
          <key-binding alias="status" key="inStatus"/>
          <key-binding alias="&quot;This username is already in use.&quot;" key="itemCode"/>
          <key-binding alias="0" key="itemStatus"/>
          <key-binding alias="status" key="status"/>
        </pipelet-node>
        <node-display orientation="horizontal" x="2" y="0"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="1" y="0"/>
        </transition-display>
      </simple-transition>
      <node>
        <jump-node start-name-ref="mAccount-Register"/>
        <node-display orientation="horizontal" x="1" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_5">
    <segment>
      <node>
        <text-node>
          <description>Check registration file</description>
        </text-node>
        <node-display x="8" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="CheckRegistration">
    <segment>
      <node>
        <start-node name="CheckRegistration" secure="false"/>
        <node-display x="8" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="mobile/account/login/CheckRegistration.ds"/>
          <key-binding alias="scriptLog" key="ScriptLog"/>
          <key-binding alias="CurrentHttpParameterMap" key="map"/>
          <key-binding alias="status" key="status"/>
          <key-binding alias="tmp" key="tmp"/>
          <key-binding alias="CurrentForms.profile" key="RegistrationForm"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="1" y="0"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <interaction-node transaction-required="false">
                <template buffered="true" dynamic="false" name="mobile/account/user/registration"/>
              </interaction-node>
              <node-display orientation="horizontal" x="1" y="0"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <jump-node start-name-ref="mAccount-CreateAccount"/>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_7">
    <segment>
      <node>
        <text-node>
          <description>Updates the profile of an authenticated customer.</description>
        </text-node>
        <node-display x="13" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="EditProfile">
    <segment>
      <node>
        <start-node name="EditProfile" secure="true"/>
        <node-display x="13" y="2"/>
      </node>
      <simple-transition/>
      <node>
        <call-node start-name-ref="mAccount-RequireLogin"/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
          <key-binding alias="CurrentForms.profile" key="FormElement"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node custom-name="Profile" pipelet-name="UpdateFormWithObject" pipelet-set-identifier="bc_api">
          <config-property key="Clear" value="false"/>
          <key-binding alias="CurrentForms.profile.customer" key="Form"/>
          <key-binding alias="CurrentCustomer.profile" key="Object"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node custom-name="Credentials" pipelet-name="UpdateFormWithObject" pipelet-set-identifier="bc_api">
          <config-property key="Clear" value="false"/>
          <key-binding alias="CurrentForms.profile.login" key="Form"/>
          <key-binding alias="CurrentCustomer.profile.credentials" key="Object"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node custom-name="AddressBook" pipelet-name="UpdateFormWithObject" pipelet-set-identifier="bc_api">
          <config-property key="Clear" value="false"/>
          <key-binding alias="CurrentForms.profile.addressbook.addresses" key="Form"/>
          <key-binding alias="CurrentCustomer.profile.addressBook.addresses" key="Object"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <key-binding alias="null" key="From_0"/>
          <key-binding alias="Status" key="To_0"/>
          <key-binding alias="null" key="From_1"/>
          <key-binding alias="null" key="To_1"/>
          <key-binding alias="null" key="From_2"/>
          <key-binding alias="null" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="null" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <interaction-continue-node secure="true" start-name="SubmitForm3" transaction-required="false">
          <template buffered="true" dynamic="false" name="mobile/account/user/registration"/>
        </interaction-continue-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="cancel">
          <transition target-connector="in"/>
          <segment>
            <node>
              <pipelet-node pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
                <key-binding alias="CurrentForms.profile" key="FormElement"/>
              </pipelet-node>
              <node-display x="-1" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <jump-node start-name-ref="mAccount-Show"/>
              <node-display x="0" y="1"/>
            </node>
          </segment>
        </branch>
        <branch basename="b3" source-connector="confirm">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="target" x="0" y="-1"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <call-node start-name-ref="mAccount-CheckUserName"/>
              <node-display x="0" y="2"/>
              <branch basename="b2" source-connector="error">
                <transition target-connector="in">
                  <transition-display>
                    <bend-point relative-to="target" x="-1" y="0"/>
                  </transition-display>
                </transition>
                <segment>
                  <node>
                    <pipelet-node pipelet-name="InvalidateFormElement" pipelet-set-identifier="bc_api">
                      <key-binding alias="CurrentForms.profile.customer.email" key="FormElement"/>
                    </pipelet-node>
                    <node-display orientation="horizontal" x="1" y="1"/>
                  </node>
                  <transition target-connector="in2" target-path="./+1"/>
                </segment>
                <segment>
                  <node>
                    <join-node/>
                    <node-display x="1" y="0"/>
                  </node>
                  <transition target-connector="in1" target-path="../../+1"/>
                </segment>
              </branch>
              <branch basename="b3" source-connector="ok">
                <transition target-connector="in"/>
                <segment>
                  <node>
                    <decision-node condition-key="CurrentForms.profile.customer.email.value == CurrentForms.profile.customer.emailconfirm.value" condition-operator="expr"/>
                    <node-display x="0" y="2"/>
                    <branch basename="b2" source-connector="yes">
                      <transition target-connector="in">
                        <transition-display>
                          <bend-point relative-to="source" x="0" y="1"/>
                        </transition-display>
                      </transition>
                      <segment>
                        <node>
                          <decision-node condition-key="CurrentForms.profile.login.password.value==CurrentForms.profile.login.passwordconfirm.value" condition-operator="expr"/>
                          <node-display x="0" y="1"/>
                          <branch basename="b2" source-connector="yes">
                            <transition target-connector="in">
                              <transition-display>
                                <bend-point relative-to="source" x="0" y="1"/>
                              </transition-display>
                            </transition>
                            <segment>
                              <node>
                                <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                                  <config-property key="Transactional" value="false"/>
                                  <key-binding alias="CurrentForms.profile.customer.email.value" key="From_0"/>
                                  <key-binding alias="CurrentForms.profile.login.username.value" key="To_0"/>
                                  <key-binding alias="null" key="From_1"/>
                                  <key-binding alias="null" key="To_1"/>
                                  <key-binding alias="null" key="From_2"/>
                                  <key-binding alias="null" key="To_2"/>
                                  <key-binding alias="null" key="From_3"/>
                                  <key-binding alias="null" key="To_3"/>
                                  <key-binding alias="null" key="From_4"/>
                                  <key-binding alias="null" key="To_4"/>
                                  <key-binding alias="null" key="From_5"/>
                                  <key-binding alias="null" key="To_5"/>
                                  <key-binding alias="null" key="From_6"/>
                                  <key-binding alias="null" key="To_6"/>
                                  <key-binding alias="null" key="From_7"/>
                                  <key-binding alias="null" key="To_7"/>
                                  <key-binding alias="null" key="From_8"/>
                                  <key-binding alias="null" key="To_8"/>
                                  <key-binding alias="null" key="From_9"/>
                                  <key-binding alias="null" key="To_9"/>
                                </pipelet-node>
                                <node-display x="0" y="1"/>
                              </node>
                              <simple-transition transaction-control="begin">
                                <transition-display>
                                  <bend-point relative-to="source" x="0" y="1"/>
                                </transition-display>
                              </simple-transition>
                              <node>
                                <pipelet-node pipelet-name="UpdateObjectWithForm" pipelet-set-identifier="bc_api">
                                  <key-binding alias="CurrentForms.profile.login" key="Form"/>
                                  <key-binding alias="CurrentCustomer.profile.credentials" key="Object"/>
                                </pipelet-node>
                                <node-display x="0" y="1"/>
                                <branch basename="b2" source-connector="error">
                                  <transition target-connector="in1" target-path="./b4.1" transaction-control="rollback">
                                    <transition-display>
                                      <bend-point relative-to="source" x="2" y="0"/>
                                    </transition-display>
                                  </transition>
                                </branch>
                              </node>
                              <simple-transition>
                                <transition-display>
                                  <bend-point relative-to="source" x="0" y="1"/>
                                </transition-display>
                              </simple-transition>
                              <node>
                                <pipelet-node pipelet-name="SetCustomerPassword" pipelet-set-identifier="bc_api">
                                  <key-binding alias="CurrentForms.profile.login.password.value" key="Password"/>
                                  <key-binding alias="CurrentCustomer" key="Customer"/>
                                </pipelet-node>
                                <node-display x="0" y="1"/>
                                <branch basename="b3" source-connector="error">
                                  <transition target-connector="in1" target-path="./b4.1" transaction-control="rollback"/>
                                </branch>
                              </node>
                              <simple-transition>
                                <transition-display>
                                  <bend-point relative-to="source" x="0" y="1"/>
                                </transition-display>
                              </simple-transition>
                              <node>
                                <pipelet-node pipelet-name="UpdateObjectWithForm" pipelet-set-identifier="bc_api">
                                  <key-binding alias="CurrentForms.profile.customer" key="Form"/>
                                  <key-binding alias="CurrentCustomer.profile" key="Object"/>
                                </pipelet-node>
                                <node-display x="0" y="1"/>
                                <branch basename="b4" source-connector="error">
                                  <transition target-connector="in1" transaction-control="rollback">
                                    <transition-display>
                                      <bend-point relative-to="source" x="2" y="0"/>
                                    </transition-display>
                                  </transition>
                                  <segment>
                                    <node>
                                      <join-node/>
                                      <node-display x="1" y="-1"/>
                                    </node>
                                    <transition target-connector="in1" target-path="../../+1">
                                      <transition-display>
                                        <bend-point relative-to="source" x="2" y="0"/>
                                      </transition-display>
                                    </transition>
                                  </segment>
                                </branch>
                              </node>
                              <simple-transition transaction-control="commit"/>
                              <node>
                                <pipelet-node pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
                                  <key-binding alias="CurrentForms.profile" key="FormElement"/>
                                </pipelet-node>
                                <node-display x="0" y="2"/>
                              </node>
                              <simple-transition/>
                              <node>
                                <jump-node start-name-ref="mAccount-Show"/>
                                <node-display x="0" y="1"/>
                              </node>
                            </segment>
                          </branch>
                        </node>
                        <simple-transition/>
                        <node>
                          <pipelet-node pipelet-name="InvalidateFormElement" pipelet-set-identifier="bc_api">
                            <key-binding alias="CurrentForms.profile.login.passwordconfirm" key="FormElement"/>
                          </pipelet-node>
                          <node-display orientation="horizontal" x="1" y="0"/>
                        </node>
                        <transition target-connector="in3" target-path="./+1"/>
                      </segment>
                      <segment>
                        <node>
                          <join-node/>
