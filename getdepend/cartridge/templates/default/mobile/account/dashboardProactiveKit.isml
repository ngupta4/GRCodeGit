<iscomment><isif condition="${!empty(dw.web.Resource.msg('kitcustomizer.isenabled','common',null)) && dw.web.Resource.msg('kitcustomizer.isenabled','common',null)=='true'}">         <div class="title">
            <isprint value="${Resource.msg('account.wenkit','account',null)}"/>
          </div>
          <div class="panelWraper">          
            <div class="row">                        <isif condition="${'kitProducts' in pdict.CurrentCustomer.profile.custom && !empty(pdict.CurrentCustomer.profile.custom.kitProducts)}"> 	            <isscript>	    			var products : Array = pdict.CurrentCustomer.profile.custom.kitProducts;					var pidArray : Array = new Array();						if(products != null && !empty(products)){						for(var i=0; i<products.length; i++){							var tempArray : Array = new Array();							if(products[i].indexOf(dw.web.Resource.msg('symbol.kit.split','common',null))>=0){								tempArray = products[i].split(dw.web.Resource.msg('symbol.kit.split','common',null));							}							if(tempArray[1] != null)								pidArray[i] = tempArray[1].toString();							//quantityArray[i] = tempArray[2];						}					}												            </isscript>	         	   						<isloop items="${pidArray}" var="test" status="loopstate">				 <isif condition="${null!=test}">					<isprint value="${loopstate.count}"  formatter="#"/>. <isprint value="${test}" encoding="off"/>				  </isif>				</isloop>			<iselse/>					No products in kit. Please customize your kit.            </isif>                          </div>
            <div class="btn-wraper">            	<a href="${URLUtils.url('KitCustomizer-LearnMore','showKit','true')}" title="Customize Your Kit" class="brownButton aButton">            		<span><isprint value="${Resource.msg('account.customize.kit','account',null)}"/></span>            		            	</a>            </div>              
          </div></isif></iscomment><!-- Dummy Content --><div class="title">            Your Proactiv+ System          </div><div class="innerBox">    <p class="summry txtWraper">Soon you'll be able to customize your Proactiv+<sup>&reg;</sup> kit online right here, but until then, you can make changes by calling: 1-800-309-4796.</p><div class="actions">	<a class="brownBlackButton customizeKit aButton" title="" href="${URLUtils.https('KitCustomizer-KitShipDetail')}"><!-- dwMarker="link" dwPipelineTitle="Account-EditProfile (guthyrenker_businesslogic)" dwPipelineURL="http://localhost:60606/target=/c/guthyrenker_businesslogic/p/Account-EditProfile" -->               	Customize your kit						            </a>	            </div> </div>