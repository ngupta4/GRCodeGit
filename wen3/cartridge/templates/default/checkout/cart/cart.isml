                                  <iselse>
                                  <img src="${URLUtils.staticURL('/images/noimagesmall.png')}" alt="${bli.productName}" title="${bli.productName}"/> </isif>
                                </a>
                                <iselse>
                                <img src="${URLUtils.staticURL('/images/noimagesmall.png')}" alt="${bli.productName}" title="${bli.productName}"/> </isif>
                            </div>
                            <div class="name"> <a href="${URLUtils.url('Product-Show','pid', bli.productID)}" title="${bli.productName}" >
                              <isprint value="${bli.lineItemText}"/>
                              </a> </div>
                            <!-- END: name -->
                            
                            <div class="itemnumber"> <span class="label">${Resource.msg('global.itemno','locale',null)}</span> <span class="value">
                              <isprint value="${bli.productID}"/>
