                      ${Resource.msg('global.total','locale',null)}</br>
                        ${Resource.msg('global.bonus','locale',null)}
                      </div></td>
                      <iselse>
                      <isif condition="${lineItem.product != null}">
                      <isset name="PriceModel" value="${lineItem.product.getPriceModel()}" scope="page"/>
                      <isset name="Product" value="${lineItem.product}" scope="page"/>
                      
                      
                    <iscomment>Pricing changes</iscomment>
	                <isif condition="${Product.optionProduct}">
				  	 	<isset name="PriceModel" value="${Product.getPriceModel(Product.getOptionModel())}" scope="page"/>
				    <iselse/>
						<isset name="PriceModel" value="${Product.getPriceModel()}" scope="page"/>
					</isif>
					<isset name="SalesPrice" value="${PriceModel.getPriceBookPrice(dw.system.Site.current.preferences.custom.nonMemberPriceBook)}" scope="page"/> 
					<isset name="StandardPrice" value="${PriceModel.getPriceBookPrice(dw.system.Site.current.preferences.custom.nonMemberPriceBook)}" scope="page"/>
					<isif condition="${pdict.CurrentCustomer.authenticated && 'member' in  pdict.CurrentCustomer.profile.custom && pdict.CurrentCustomer.profile.custom.member}">
