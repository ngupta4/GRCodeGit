                          <isprint value="${SalesPrice}"/>
                          </span>
                          <div class="clear"><!-- FLOAT CLEAR --></div>
                        </div>
                       <iselse>
                       	    <isif condition="${StandardPrice.available && StandardPrice.equals(SalesPrice)}">
                       	    	<isif condition="${lineItem.basePrice*lineItem.quantity > lineItem.adjustedPrice}">
                       	    		<span class="strikeout"><isprint value="${lineItem.basePrice}"/></span>
