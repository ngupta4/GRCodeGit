						<isset name="salePriceBookId" value="${dw.object.CustomObjectMgr.getCustomObject('SourceCodeGroup_Pricebook_Mapping',pdict.CurrentSession.sourceCodeInfo.group.ID).custom.PriceBookID}" scope="page" />
						<isif condition="${PriceModel.getPriceBookPrice(salePriceBookId)!=dw.value.Money.NOT_AVAILABLE}">
							<div class="salesprice">
							<isset name="SalesPrice" value="${PriceModel.getPriceBookPrice(salePriceBookId)}"></isset></div>														
						</isif>	
					</isif>
					 ${Resource.msg('global.yourprice','locale',null)}
					  <isset name="ShowStandardPrice" value="${false}" scope="page"/>
                      <isif condition="${StandardPrice.available && StandardPrice > SalesPrice}">
                        <div class="standardprice">
                          <div class="label"><iscomment>${Resource.msg('cart.listprice','checkout',null)}:</iscomment></div>
                          
