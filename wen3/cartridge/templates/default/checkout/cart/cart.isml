                            <isset name="quantity" value="${pdict.Basket.getAllProductQuantities().get(lineItem.product).value}" scope="page"/>
                            <isinclude template="checkout/cart/cartavailability"/>
                          </isif>
                        </div>
                        <!-- END: stockstate --></td>
                    </tr>
