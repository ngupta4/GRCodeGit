                            <td class="itemtotalcolumn"><!-- ITEMTOTAL COLUMN --></td>
                          <td class="quantitycolumndetails"> ${Resource.msg('global.included','locale',null)}
                            <iscomment>Product Existence & Product Availability</iscomment>
