						<td><!-- BLANK IMAGE CELL --></td>
						<td class="detailscolumn">
							<div class="cartcoupon">
								<span class="label">${Resource.msg('cart.promocode','checkout',null)}</span>
								<span class="value"><isprint value="${FormCoupon.object.couponCode}"/></span>
								<div class="clear"><!-- FLOAT CLEAR --></div>
							</div>
							<isscript>
								/* var couponLineItem : dw.order.CouponLineItem = FormCoupon.object;
								var description : String = "";
								if (couponLineItem != null) {
									for(PriceAdjustment pa  in couponLineItem.priceAdjustments)	{
									    Money couponDiscount = pa.getPrice();
									}
								}*/

								/* jfriend 103113 - resolving quota violations */
								var promoCalloutMsg : String = "";
								var promoClass : String = "";
								var couponLineItem : dw.order.CouponLineItem = FormCoupon.object;
								if( !empty( couponLineItem ) ) {
									var coupon : Coupon = dw.campaign.CouponMgr.getCouponByCode( couponLineItem.couponCode );
									if( !empty( coupon ) ) {
										var promos : ArrayList = dw.campaign.PromotionMgr.getActiveCustomerPromotions().promotions;
										for each( var promo : dw.campaign.Promotion in promos ) {
											if( promo.coupons.contains( coupon ) ) {
												promoCalloutMsg = promo.calloutMsg;
												promoClass = promo.promotionClass;
												break;
											}
										}
									}
								}
							</isscript>
							
							<iscomment>description</iscomment>
							<isif condition="${!empty(promoCalloutMsg)}">
								<div><isprint value="${promoCalloutMsg}"/></div>
							</isif>
							
							<isif condition="${FormCoupon.object.couponCode == pdict.CurrentSession.sourceCodeInfo.group.ID}">
								<iscomment>Do not show the remove link</iscomment>	
							<iselse/>
								<button class="textbutton" type="submit" value="${Resource.msg('global.remove','locale',null)}" name="${FormCoupon.deleteCoupon.htmlName}"><span>${Resource.msg('global.remove','locale',null)}</span></button>
							</isif>
							
							<isif condition="${!FormCoupon.object.valid}">
