                                <iscomment>Changed Quantity textbox to dropdown</iscomment>
		                        <input class="pid" type="hidden" value="${lineItem.product.ID}">
		                        <select class="selectQuantity" name="${FormLi.quantity.htmlName}" disabled="disabled">
		                          <isif condition="${lineItem.product.custom.maxQty != null}">
		                            <isset name="maxQtyValue" value="${Number(lineItem.product.custom.maxQty)}" scope="page"/>
		                            <isset name="minQtyValue" value="${1}" scope="page"/>
		                            <isloop status="qty" begin="${minQtyValue}" end="${maxQtyValue}">
		                      		  <isif condition="${qty.count == lineItem.quantity.value}">
		                                <option value="${qty.count}" selected="selected">${qty.count}</option>
		                                <iselse>
		                                <option value="${qty.count}">${qty.count}</option>
		                              </isif>
		                            </isloop>
		                          </isif>
		                        </select>
                      	</isif>
                     </td>
                   
                    <td  <isif condition="${lineItem.bonusProductLineItem}"> class="itemtotalcolumn bonusProduct" <iselse/>class="itemtotalcolumn"</isif>>
                    <div class="itemtotals">
                     
