	                          <isset name="thumbnailUrl" value="${URLUtils.staticURL('/images/noimagemedium.png')}" scope="page"/>
	                    </isif>
	                    <isset name="thumbnailAlt" value="${lineItem.product.name}" scope="page"/>
	                    <isset name="thumbnailTitle" value="${lineItem.product.name}" scope="page"/>                            
	
	                    <p class="productimage">
	                          <a href="${URLUtils.url('Product-Show','pid', lineItem.product.ID)}" title="${lineItem.product.name}"><img src="${thumbnailUrl}" alt="${thumbnailAlt}" title="${thumbnailTitle}" /></a>
	                    </p>
	                    <iscomment>END : Picks the default product thumbnail from scene 7</iscomment>
	                    
	                    <iscomment>
		                    <isif condition="${lineItem.bonusProductLineItem}"> 
		                    	<span class="bonusitem">${Resource.msg('global.bonus','locale',null)}</span> 
