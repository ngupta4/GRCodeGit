<isdecorate template="checkout/cart/pt_cart">
<input type="hidden" id="tagetPipeline" value="${pdict.TargetPipeline}"/>
<input type="hidden" id="tagetParams" value="${pdict.TargetPipelineParams}"/>
  <isinclude template="util/modules"/>
  <isinclude template="util/reporting/ReportBasket.isml"/>
  <script type="text/javascript"><!--
jQuery(document).ready(function(){
	jQuery(document).bind("refreshCart", app.refreshCart);
	
	// overwrite MiniCart hook
	app.minicart.suppressSlideDown = function()
	{
		// suppress slide down of mini cart
		return true;
	}

	jQuery(".editdetails a").click(function(){
		var url = "${URLUtils.httpContinue()}";
		// append the continue node next action which is editLineItem in this case
		url = app.util.appendParamToURL(url, this.name, "");
		
		// unbind any existing event handlers
		jQuery(document).unbind("AddToCart");
		// bind to AddToCart event, it gets triggered when the user has made selections
		// on the product detail page and clicked A2C button
		// event.selectedOptions contain name/value pair for the selected produt data e.g. pid, Quantity, any options
		jQuery(document).bind("AddToCart", function(event){
			var pliOptions = jQuery.extend({}, {}, event.selectedOptions);
			
			// make server call to replace the lineitem and refresh the cart
			// update the product
			jQuery.ajax({
						type	: "POST",
						url		: url,
						cache	: false,
						data	: pliOptions,
						success	: function(response){ jQuery("#loader").html(response);},
						error	: function(){app.refreshCart();}
						});
		});
		
		app.quickView.show({url: this.href, source: "cart", label: "<span class='lc' style='margin-top:-1px;'></span><span class='updatebuttton'>Update</span>"});

		
		

		return false;
	});

	jQuery(".bonusproducts a").click(function(){
		app.bonusProductsView.show({url: this.href, source: "cart", label: "Update"});
		return false;
	});
	jQuery("#remove_product").click(function() {
		s.linkTrackVars='pageName,events';
   		s.events="scRemove";
   		s.pageName='proactiv:scp:checkout';
   		s.tl(this,'o',"Remove product");
	});
	
});

//--></script>
  
  <iscomment>CREATES SCRIPT TO RUN THROUGH ODD/EVEN ROWS COLOR </iscomment>
  <isscript> /*
    Small function to realize odd and even recognition of rows across multiple
    loops
    */
    
    function RowClass()
    {
    // define the counter
    var rowCounter = 0;
    
    RowClass.prototype.getName = function()
    {
    // increase counter
    rowCounter++;
    
    if (rowCounter % 2 == 0)
    {
    return 'even';
    }
    else
    {
    return 'odd';
    }
    }
    };
  
 </isscript>
  <isscript> /* create a new instance to name our rows 'odd' or 'even' */
    var rowNaming = new RowClass(); </isscript>
  <isset name="enableCheckout" value="${pdict.EnableCheckout}" scope="page"/>
  <div class="cart">
  <h1>${Resource.msg('cart.header','checkout',null)}</h1>
	<isif condition = "${!empty(pdict.CurrentHttpParameterMap.error.stringValue)}">
		<div id="errorCode" style="color:red; font-size:12px; padding-top:4px;">
			<isprint value="${Resource.msg('autologin.'+pdict.CurrentHttpParameterMap.error.stringValue, 'errormessages',null)}"/>
		</div>
	</isif>	
  <isif condition="${pdict.Basket == null || (empty(pdict.Basket.productLineItems) && empty(pdict.Basket.giftCertificateLineItems))}">
   
    <isslot id="cart-banner" description="Banner for Cart page" context="global" />
    
    <h2>${Resource.msg('cart.cartempty','checkout',null)}</h2>
    <iscomment>
    <p>${Resource.msg('cart.previousadd','checkout',null)}</p>
    </iscomment>
     <form class="formcontinueshopping" action="${URLUtils.httpContinue()}" method="post" id="${pdict.CurrentForms.cart.dynamicHtmlName}">
              <div class="continueshopping"><fieldset>
                <button class="addtocartbutton cartGreenBtn" type="submit" value="${Resource.msg('global.continueshopping','locale',null)}" name="${pdict.CurrentForms.cart.continueShopping.htmlName}"><span class="lc"></span><span>${Resource.msg('global.continueshopping','locale',null)}</span></button>
              </fieldset></div>
     </form>
    <iselse> 
   
   
      <iscomment>prevent checkout if any product price is N/A or if invalid coupons</iscomment>
      <isif condition="${pdict.BasketStatus != null && pdict.BasketStatus.error}">
        <div class="errorform ">
          <isif condition="${pdict.BasketStatus.code != null && pdict.BasketStatus.code=='CouponError'}"> ${Resource.msg('cart.cartcouponinvalid','checkout',null)}
            <iselseif condition="${pdict.BasketStatus.code != null && pdict.BasketStatus.code=='TaxError'}">
            ${Resource.msg('cart.taxinvalid','checkout',null)}
            <iselse>
            ${Resource.msg('cart.carterror','checkout',null)} </isif>
        </div>
      </isif>
      <div class="actions">
        <iscomment>continue shop url is a non-secure but checkout needs a secure and that is why separate forms!</iscomment>
        <ul>
        	
          <li>
            <form class="formcontinueshopping" action="${URLUtils.httpContinue()}" method="post" id="${pdict.CurrentForms.cart.dynamicHtmlName}">
              <fieldset>
                <button class="blue-btn cartGreenBtn" type="submit" value="${Resource.msg('global.continueshopping','locale',null)}" name="${pdict.CurrentForms.cart.continueShopping.htmlName}">
                <span class="lc"></span>
                <span>${Resource.msg('global.continueshopping','locale',null)}</span></button>
              </fieldset>
            </form>
          </li>
          <li>
            <form class="formcheckout" action="${URLUtils.httpsContinue()}" method="post" id="${pdict.CurrentForms.cart.dynamicHtmlName}">
              <fieldset>
                <isif condition="${enableCheckout}">
                  <button class="gr-btn continue" type="submit" value="${Resource.msg('global.checkout','locale',null)}" name="${pdict.CurrentForms.cart.checkoutCart.htmlName}"><span class="lc"></span><span>${Resource.msg('global.proceedtocheckout','locale',null)}</span></button>
                  <iselse>
                  <button class="imagebutton continuecheckout disabled" disabled="disabled" type="submit" value="${Resource.msg('global.checkout','locale',null)}" name="${pdict.CurrentForms.cart.checkoutCart.htmlName}"></button>
                </isif>
              </fieldset>
            </form>
          </li>
        </ul>
        <div class="clear"><!-- FLAOT CLEAR --></div>
      </div>
      <iscomment>Display selected bonus products Order Promotion message</iscomment>
      <isloop items="${pdict.Basket.bonusDiscountLineItems}" var="bonusDiscountLineItem">
        <isif condition="${bonusDiscountLineItem.getPromotion().getPromotionClass() == dw.campaign.Promotion.PROMOTION_CLASS_ORDER && bonusDiscountLineItem.getBonusProductLineItems().size() == 0}">
          <div class="orderbonuslineitempromo">
            <iscomment>Display appropriate text based on status of bonus discount and number of selected bonus products.</iscomment>
            <isif condition="${bonusDiscountLineItem.getBonusProductLineItems().size() > 0}">
              <isif condition="${bonusDiscountLineItem.getMaxBonusItems() > 1}">
                <isset name="bonusButtonText" value="${Resource.msg('cart.updatebonusproducts','checkout',null)}" scope="page"/>
                <iselse>
                <isset name="bonusButtonText" value="${Resource.msg('cart.updatebonusproduct','checkout',null)}" scope="page"/>
              </isif>
              <iselse>
              <isif condition="${bonusDiscountLineItem.getMaxBonusItems() > 1}">
                <isset name="bonusButtonText" value="${Resource.msg('cart.selectbonusproducts','checkout',null)}" scope="page"/>
                <iselse>
                <isset name="bonusButtonText" value="${Resource.msg('cart.selectbonusproduct','checkout',null)}" scope="page"/>
              </isif>
            </isif>
            <div class="bonuslineitemdetails">
              <div class="promo">
                <isprint value="${bonusDiscountLineItem.getPromotion().getName()}"/>
              </div>
              <isprint value="${bonusDiscountLineItem.getPromotion().getCalloutMsg()}"/>
              <span class="formfieldtooltip"><a class="tooltip">${Resource.msg('product.promotiondetails','product',null)}
              <div class="tooltip-body" style="display:none">
                <div class="tooltipcontainer">
                  <isprint value="${bonusDiscountLineItem.getPromotion().getDetails()}"/>
                  <br />
                  ${StringUtils.format(Resource.msg('cart.bonusmaxitems','checkout',null), bonusDiscountLineItem.getMaxBonusItems())} ${StringUtils.format(Resource.msg('cart.bonusnumselected','checkout',null), bonusDiscountLineItem.getBonusProductLineItems().size())} </div>
              </div>
              </a> </span> </div>
            <div class="bonusproducts"> <a href="${URLUtils.url('Product-GetBonusProducts','bonusDiscountLineItemUUID', bonusDiscountLineItem.UUID)}" title="${bonusButtonText}" class="imagebutton"><span>${bonusButtonText}</span></a> </div>
            <div class="clear"><!-- FLOAT CLEAR --></div>
          </div>
        </isif>
      </isloop>
 
	  <div class="clear"><!-- FLOAT CLEAR --></div>
      <iscomment>show the basket when we have data </iscomment>
      <form action="${URLUtils.httpContinue()}" method="post" id="${pdict.CurrentForms.cart.htmlName}">
