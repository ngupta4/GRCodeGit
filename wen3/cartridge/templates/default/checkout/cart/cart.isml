                        </isif>
                        <iselse>
                        ${Resource.msg('global.iteminwishlist','locale',null)} <br />
                      </isif>
                      <a class="addtoregistry" href="${URLUtils.https('GiftRegistry-AddProduct', 'pid', lineItem.productID)}" name="${FormLi.addToGiftRegistry.htmlName}" title="${Resource.msg('global.addtogiftregistry','locale',null)}">${Resource.msg('global.addtogiftregistry','locale',null)}</a><br />
                      <!-- Product Existence & Product Availability-->
                      <isif condition="${!lineItem.bonusProductLineItem || lineItem.getBonusDiscountLineItem() != null}">
                        <div class="stockstate">
                          <isif condition="${lineItem.product == null}"> <span class="notavailable"> ${Resource.msg('cart.removeditem','checkout',null)} </span>
                            <iselse>
                            <isset name="product" value="${lineItem.product}" scope="page"/>
                            <isset name="quantity" value="${pdict.Basket.getAllProductQuantities().get(lineItem.product).value}" scope="page"/>
                            <isinclude template="checkout/cart/cartavailability"/>
                          </isif>
                        </div>
                        <!-- END: stockstate --> 
                      </isif></iscomment>
                      
                     
                      <isloop items="${lineItem.priceAdjustments}" var="pli" status="prAdloopstatus">
                      <isset name="LineItemPrice" value="${LineItemPrice.add(pli.getPrice().divide(lineItem.getQuantity().getValue()))}" scope="page"/>
                      <isif condition="${lineItem.priceAdjustments.length > 1}">
                      <div class="priceadjustment">
                      	<iscomment>Removing promotion name and promo price, to match latest PSD</iscomment>
                        <iscomment><div class="promo">
                          <isprint value="${pli.promotion.name}"/>
                        </div>
                        
                        <div class="promoprice"> <span class="label">${Resource.msg('cart.promoprice','checkout',null)}:</span> <span class="value">
                          <isprint value="${LineItemPrice}"/>
                          </span>
                          <div class="clear"><!-- FLOAT CLEAR --></div>
                        </div></iscomment>
                      </div>
                      <iselse>
                      <iscomment>Removing promotion name and promo price, to match latest PSD</iscomment>
                      <iscomment><div class="promo">
                        <isprint value="${pli.promotion.name}"/>
                      </div></iscomment>
                      <isif condition="${lineItem.quantity > 1}">
                      <iscomment><div class="promoprice"> <span class="label">${Resource.msg('cart.promoprice','checkout',null)}:</span>
                        <div class="value">
                          <isprint value="${LineItemPrice}"/>
                        </div>
                        <div class="clear"><!-- FLOAT CLEAR --></div>
                      </div></iscomment>
                      <div class="itemtotal"> 
                      <iscomment><span class="label">x
                        <isprint value="${lineItem.quantity}"/>
                        </span></iscomment>
                       <iscomment> <iselse>
                        <div class="itemtotal"> <span class="label">${Resource.msg('cart.itemtotal','checkout',null)}:</span></iscomment>
                          </isif>
                          <span class="value">
                          <isprint value="${lineItem.getAdjustedPrice()}"/>
                          </span> </div>
                        <!-- END: itemtotal -->
                        </isif>
                        </isloop>
                        <isif condition="${lineItem.priceAdjustments && lineItem.priceAdjustments.length != 1}">
                          <div class="itemtotal">
                            <isif condition="${lineItem.quantity > 1}"> <span class="label">
                            <iscomment>x
                              <isprint value="${lineItem.quantity}"/></iscomment>
                              </span>
                             <iscomment> <iselseif condition="${ShowStandardPrice}">
                              <span class="label">${Resource.msg('cart.itemtotal','checkout',null)}:</span></iscomment> </isif>
                            <span class="value">
                            <isprint value="${lineItem.getAdjustedPrice()}"/>
                            </span> </div>
                        </isif>
                        <isif condition="${lineItem.shippingLineItem != null}">
                          <isif condition="${lineItem.shippingLineItem.surcharge}">
                            <div> <span class="label">
                              <isprint value="${Resource.msg('singleshipping.surcharge','checkout',null)}"/>
                              :</span> <span class="value">
                              <isprint value="${lineItem.shippingLineItem.getAdjustedPrice()}"/>
                              </span> </div>
                          </isif>
                          <iselse/>
                          <isscript> var defaultMethod : dw.order.ShippingMethod = dw.order.ShippingMgr.getDefaultShippingMethod();
                            var shippingModel : dw.order.ProductShippingModel = dw.order.ShippingMgr.getProductShippingModel(lineItem.getProduct());
                            var shippingCost : dw.order.ProductShippingCost = shippingModel.getShippingCost(defaultMethod); </isscript>
                          <isif condition="${!empty(shippingCost) && shippingCost.surcharge}">
                            <div> <span class="label">
                              <isprint value="${Resource.msg('singleshipping.surcharge','checkout',null)}"/>
                              :</span> <span class="value">
                              <isprint value="${shippingCost.amount}"/>
                              </span> </div>
                          </isif>
                        </isif>
                        </isif>
                        </isif>
                        <isif condition="${lineItem.optionProductLineItems.size() > 0}">
                          <isloop items="${lineItem.optionProductLineItems}" var="oli">
                            <isif condition="${oli.price > 0}">
                              <div class="optionprice"> <span class="label">${Resource.msg('cart.option','checkout',null)}:</span> <span class="value">+
                                <isprint value="${oli.price}"/>
                                </span>
                                <div class="clear"><!-- FLOAT CLEAR --></div>
                              </div>
                              <isloop items="${oli.priceAdjustments}" var="pli">
                                <p class="priceadjustment">
                                  <isprint value="${pli.price}"/>
                                </p>
                              </isloop>
                            </isif>
                          </isloop>
                        </isif>
                      </div>
                  	</td>
                  	
                  </tr>
                  
                  <tr class="blankspace_bottom">
                    <td colspan="5">&nbsp;</td>
                  </tr>
                  
